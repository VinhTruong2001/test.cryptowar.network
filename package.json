{
  "name": "kryptoknights",
  "version": "1.0.0",
  "description": "",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.1.7",
    "@openzeppelin/contracts": "^3.4.1",
    "@openzeppelin/contracts-upgradeable": "^3.4.1",
    "@truffle/hdwallet-provider": "^1.2.6",
    "abdk-libraries-solidity": "2.4.0",
    "censor-sensor": "^1.0.6",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@openzeppelin/test-helpers": "^0.5.11",
    "@openzeppelin/truffle-upgrades": "^1.8.0",
    "@truffle/config": "^1.2.41",
    "@truffle/provider": "^0.2.32",
    "bignumber.js": "^9.0.1",
    "graph-data-structure": "^2.0.0",
    "imagemin": "^8.0.0",
    "imagemin-pngquant": "^9.0.2",
    "lerna": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "ohm-js": "^15.5.0",
    "shelljs": "^0.8.4",
    "truffle": "^5.2.3",
    "truffle-contract-size": "^2.0.1",
    "truffle-export-abi": "^1.0.1",
    "truffle-plugin-verify": "^0.5.7",
    "yargs": "^17.0.1"
  },
  "scripts": {
    "postinstall": "npm run contract:prepare && lerna exec -- npm install",
    "lint": "lerna exec -- npm run lint",
    "build:frontend": "cd frontend && npm run build",
    "start:frontend": "cd frontend && npm start",
    "contract:prepare": "npm run build",
    "contract:deploy": "npm run deploy",
    "build": "truffle compile",
    "postbuild": "node scripts/generate-ts-from-abis.js",
    "test": "truffle test",
    "deploy": "truffle migrate --reset",
    "deploy-testnet": "truffle migrate --network bsctestnet",
    "postdeploy-testnet": "run-s \"backup-networks 97\" \"verify-contracts -- --network bsctestnet\"",
    "deploy-mainnet-warning-be-careful-this-is-prod": "truffle migrate --network bscmainnet",
    "postdeploy-mainnet-warning-be-careful-this-is-prod": "run-s \"backup-networks 56\" \"verify-contracts -- --network bscmainnet\"",
    "deploy-mainnet-dry-run": "truffle migrate --network bscmainnet --dry-run",
    "backup-networks": "node scripts/manage-networks.js backup",
    "verify-contracts": "node scripts/verify-contracts.js",
    "deploy:xblade": "truffle migrate -f 1 --to 1 --network bscmainnet",
    "deploy:randomness": "truffle migrate -f 6 --to 6 --network bscmainnet",
    "deploy:cryptowar": "truffle compile --all && truffle migrate -f 11 --to 11 --network bscmainnet",
    "upgrade:xblade": "truffle compile --all && truffle migrate -f 40 --to 40 --network bscmainnet",
    "upgrade:cryptowars": "truffle compile --all && truffle migrate -f 41 --to 41 --network bscmainnet",
    "upgrade:weapon": "truffle compile --all && truffle migrate -f 42 --to 42 --network bscmainnet",
    "verify:xblade": "npx truffle run verify xBlade@0x488c0F82C3c3b8260B4ac21264274780DE033bDD --network bscmainnet",
    "verify:randomness": "npx truffle run verify ChainlinkRandoms@0xBDCBD31a02718FA8f0c356265aaBB1D2f4000A26 --network bscmainnet",
    "verify:Weapons": "npx truffle run verify Weapons@0xa7f00d7272Bb1D410cEC203B4b8077D99077b515 --network bscmainnet",
    "verify:CryptoWars": "npx truffle run verify CryptoWars@0x99982CAa45F42d6C67f69C71069788300F1e2aFa --network bscmainnet",
    "verify:Blacksmith": "npx truffle run verify Blacksmith@0xdD3493dEcAC2bD82391fd6fd2f3a6c983372a015 --network bscmainnet",
    "verify:Promos": "npx truffle run verify Promos@0x3289d71fc107bb2a686879e35895bae6634a1df9 --network bscmainnet",
    "verify:RaidBasic": "npx truffle run verify RaidBasic@0x8087ee61FC0c42d4A39f3ec78Bf69b245A684880 --network bscmainnet",
    "verify:NFTMarket": "npx truffle run verify NFTMarket@0xfe50D30f787BfAD9296332C86F4bf8252c3E2c1a --network bscmainnet",
    "verify:SecretBox": "npx truffle run verify SecretBox@0xe474F205d4FBDd21C0b1E0E709776E46a6722b84 --network bscmainnet",
    "verify:Shields": "npx truffle run verify Shields@0xBe67D7Dbf14967109dFDB6bFCDE8C57bb76F78BB --network bscmainnet",
    "verify:XBladeStakingRewardsUpgradeable": "npx truffle run verify XBladeStakingRewardsUpgradeable@0x80ceb112dEfDeE0626969c6c71CE9Fd3D1beBBdA --network bscmainnet",
    "verify:Characters": "npx truffle run verify Characters@0xaC5CceF4629bff451867edcD86c31dEE0864A242 --network bscmainnet"
  },
  "keywords": [],
